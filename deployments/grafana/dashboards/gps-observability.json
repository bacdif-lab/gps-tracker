{
  "uid": "gps-observability",
  "title": "GPS Tracker Observabilidad",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "type": "stat",
      "title": "Ingestiones por minuto",
      "id": 1,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(gps_ingestions_total[1m]))",
          "legendFormat": "ingest/min",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "stat",
      "title": "Latencia API p95 (s)",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
        "targets": [
          {
          "expr": "histogram_quantile(0.95, sum(rate(gps_api_latency_seconds_bucket[5m])) by (le, path))",
          "legendFormat": "{{path}}",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal"
      }
    },
    {
      "type": "timeSeries",
      "title": "Alertas entregadas/errores",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "increase(gps_alert_notifications_total[5m])",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "lines",
            "lineInterpolation": "smooth"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeSeries",
      "title": "Uptime por dispositivo",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "gps_device_online",
          "legendFormat": "{{device_id}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "lines",
            "lineInterpolation": "linear",
            "lineWidth": 2
          }
        },
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": []
  }
}
